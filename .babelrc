{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": "> 1%",
                    "node": "10"
                },
                "modules": "umd"
            }
        ]
    ],

    "plugins": [
        [
            "add-module-exports",
            {
                "addDefaultProperty": false
            }
        ]
    ]
}
